{
  "private": true,
  "name": "@login-app/misc",
  "version": "0.0.0",
  "main": "out/",
  "types": "out/",
  "scripts": {
    "build:js": "babel src --out-dir out/ --extensions .ts,.tsx --presets @babel/preset-typescript",
    "build:types": "npx tsc",
    "build": "npm run build:js && npm run build:types",
    "clean": "rm -rf out/",
    "dev": "concurrently --names 'JS  ,Type' 'npm run build:js -- --watch' 'npm run build:types -- --watch'",
    "lint": "eslint . && tsc"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "babel-loader": "^8.2.3",
    "concurrently": "^6.3.0",
    "typescript": "^4.4.4"
  }
}
