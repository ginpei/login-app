    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "concurrently": "^6.3.0",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "tailwindcss": "^2.2.19"
  }
}
